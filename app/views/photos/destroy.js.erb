$('#photos').html("<%= j render 'photos_list' %>")
<% if @claim.photos.blank? %>
  $('#photo_check').hide();
<% end %>

<% if !@claim.active && !@claim.price.blank? && !@claim.listing_name.blank? && !@claim.photos.blank? && !@claim.address.blank? %>
    $('#publish_button').attr('disabled', false);
<% else %>
    $('#publish_button').attr('disabled', true);
<% end %>
